      "bundleIdentifier": "com.ismoil793.rn-todo-app",
      "buildNumber": "1.0.0",
      "icon": "./assets/icon-ios.png"
    },
    "android": {
      "package": "com.ismoil793.rn_todo_app",
      "versionCode": 1,
      "permissions": [
        "NOTIFICATIONS"
      ]
    }
  }
}